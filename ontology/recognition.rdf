        <rdfs:label>timestamp</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://delta.linkeddata.es/def/core#ASSET -->

    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#ASSET">
        <owl:equivalentClass rdf:resource="https://w3id.org/saref#Device"/>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Aggregator -->

    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Aggregator">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/openadr#VTN"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#consumes"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#MarketSettlement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasProfile"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Profile"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasTransaction"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Transaction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Legal entities that aggregate the load or generation of various demand and/or generation/production units and aim at optimizing energy supply and consumption.</rdfs:comment>
        <rdfs:label>Aggregator</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Asset -->

    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Asset">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/openadr#Asset"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Device"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#isOwnedBy"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#BuildingManager"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Asset</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#BuildingManager -->

    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#BuildingManager">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasReward"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Reward"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#type"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#rational"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Entity that interacts in the platform</rdfs:comment>
        <rdfs:label>BuildingManager</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#BuildingOperator_ -->

    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#BuildingOperator_">
        <rdfs:subClassOf rdf:resource="http://delta.linkeddata.es/def/core#BuildingManager"/>
        <rdfs:comment>Entity that consumes or produces energy</rdfs:comment>
        <rdfs:label>buildingOperator</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Cluster -->

    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#Cluster">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasAggregatedProperty"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/saref#Property"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#clusterDescription"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#clusterName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Aggrupation of elements, such as FEIDs or Virtual Nodes</rdfs:comment>
        <rdfs:label>Cluster</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#ComfortSetting -->

    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#ComfortSetting">
        <rdfs:label>Comfort setting</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#EnergyMarket -->

    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#EnergyMarket">
        <rdfs:label>Energy Market</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#FEID -->

    <owl:Class rdf:about="http://delta.linkeddata.es/def/core#FEID">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/openadr#VEN"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Device"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#isLocatedAt"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Sector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/saref#consistsOf"/>
                <owl:someValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Asset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasComfortSetting"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#ComfortSetting"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasSystemConstraint"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#SystemConstraint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#hasTransaction"/>
                <owl:allValuesFrom rdf:resource="http://delta.linkeddata.es/def/core#Transaction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://delta.linkeddata.es/def/core#href"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Set of devices</rdfs:comment>
        <rdfs:label>FEID</rdfs:label>
    </owl:Class>
    


    <!-- http://delta.linkeddata.es/def/core#Frequency -->

