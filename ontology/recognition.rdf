<?xml version="1.0"?>
<rdf:RDF xmlns="http://delta.linkeddata.es/def/core"
     xml:base="http://delta.linkeddata.es/def/core"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:dc2="http://purl.org/dc/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:core="http://delta.linkeddata.es/def/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:saref="https://w3id.org/saref#"
     xmlns:terms="http://purl.org/dc/terms"
     xmlns:terms1="http://purl.org/dc/terms/"
     xmlns:saref4bldg="https://w3id.org/def/saref4bldg#">
    <owl:Ontology rdf:about="http://delta.linkeddata.es/def/core">
        <owl:imports rdf:resource="https://w3id.org/def/openadr"/>
        <dc:title>Recognition ontology</dc:title>
        <terms1:creator rdf:resource="https://www.iti.gr/iti/index.html"/>
        <terms1:creator>CERTH ITI</terms1:creator>
        <terms1:creator>ITI</terms1:creator>
        <terms1:license rdf:resource="http://purl.org/NET/rdflicense/cc-by4.0"/>
        <rdfs:comment>This ontology represents the  terms to allow interoperability in Recognition project. It models concepts related to enery market, smart grids and Zero enery, and it will reuse standard ontologies whenever possible.</rdfs:comment>
        <owl:versionInfo>0.1.6</owl:versionInfo>
    </owl:Ontology>
    


