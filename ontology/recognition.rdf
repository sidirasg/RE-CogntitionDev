    <!-- http://delta.linkeddata.es/def/core#clusterDescription -->

    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#clusterDescription">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>Description of a cluster</rdfs:comment>
        <rdfs:label>cluster description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#clusterName -->

    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#clusterName">
        <rdfs:comment>Name of a cluster</rdfs:comment>
        <rdfs:label>cluster name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#granularity -->

    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#granularity">
        <rdfs:comment>granularity of the profile (e.g., weekly, daily)</rdfs:comment>
        <rdfs:label>granularity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasDescription -->

    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#hasDescription">
        <rdfs:label>description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#href -->

    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#href">
        <rdfs:comment>URI of the endpoint</rdfs:comment>
        <rdfs:label>href</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#name -->

    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#name">
        <rdfs:isDefinedBy>name</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#sectorName -->

    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#sectorName">
        <rdfs:label>sector name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#status -->

    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#status">
        <rdfs:comment>The final status as if the DVN fullfiled the request or it failed</rdfs:comment>
        <rdfs:label>status</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://delta.linkeddata.es/def/core#type -->

    <owl:DatatypeProperty rdf:about="http://delta.linkeddata.es/def/core#type">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/saref#hasValue -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/saref#hasValue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>has value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/saref#timeStamp -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/saref#timeStamp">
