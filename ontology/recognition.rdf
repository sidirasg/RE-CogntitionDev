    <!-- http://delta.linkeddata.es/def/core#agreesOn -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#agreesOn">
        <rdfs:label>agrees on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#consumes -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#consumes">
        <rdfs:label>consumes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasAggregatedProperty -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasAggregatedProperty">
        <rdfs:comment>Relationship identifying the properties associated to a group of entities, such as clusters</rdfs:comment>
        <rdfs:label>has aggregated property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasComfortSetting -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasComfortSetting">
        <rdfs:label>has comfort settings</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasCurrency -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasCurrency">
        <rdfs:comment>Link between a price and its currency</rdfs:comment>
        <rdfs:label>has currency</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasDuration -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasDuration">
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:comment>Duration of an event</rdfs:comment>
        <rdfs:label>has duration</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasIncentive -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasIncentive">
        <rdfs:comment>A relation between a property and its incentives associated</rdfs:comment>
        <rdfs:label>has incentive</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasPayment -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasPayment">
        <rdfs:label>has payment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasPhase -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasPhase">
        <rdfs:label>has phase</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasProfile -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasProfile">
        <rdfs:comment>Link between an entity and its profile</rdfs:comment>
        <rdfs:label>has profile</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasReward -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasReward">
        <rdfs:domain rdf:resource="http://delta.linkeddata.es/def/core#BuildingManager"/>
        <rdfs:comment>Link between a customer and its associated reward</rdfs:comment>
        <rdfs:label>has reward</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasSmartContract -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasSmartContract">
        <rdfs:range rdf:resource="http://delta.linkeddata.es/def/core#SmartContract"/>
        <rdfs:comment>link between the Virtual node and its smart contracts</rdfs:comment>
        <rdfs:label>has smart contract</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasSystemConstraint -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasSystemConstraint">
        <rdfs:label>has system constraint</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#hasTransaction -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#hasTransaction">
        <rdfs:label>has transaction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#isAllocatedIn -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isAllocatedIn">
        <rdfs:comment>Link from a virtual node to its aggregator</rdfs:comment>
        <rdfs:label>is allocated in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#isLocatedAt -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isLocatedAt">
        <rdfs:comment>Relation between an object and the geographical feature in which it is located at</rdfs:comment>
        <rdfs:label>is located at</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#isOwnedBy -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isOwnedBy">
        <owl:inverseOf rdf:resource="http://delta.linkeddata.es/def/core#owns"/>
        <rdfs:comment>Link between a device and its customer</rdfs:comment>
        <rdfs:label>is owned by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#isRelatedToProperty -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isRelatedToProperty">
        <rdfs:label>is related to property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#isValidDuringPeriod -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#isValidDuringPeriod">
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:comment>Period where an entity, such a contract, is valid</rdfs:comment>
        <rdfs:label>is valid during period</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#manages -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#manages">
        <rdfs:comment>Relation between a Virutal Node and the  devices it manages</rdfs:comment>
        <rdfs:label>manages</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#owns -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#owns">
        <rdfs:comment>Link between the customers and the devices</rdfs:comment>
        <rdfs:label>owns</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://delta.linkeddata.es/def/core#quantifiesKPI -->

    <owl:ObjectProperty rdf:about="http://delta.linkeddata.es/def/core#quantifiesKPI">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/saref4city"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opengis.net/ont/geosparql#hasGeometry -->

    <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#hasGeometry">
        <rdfs:domain rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
        <rdfs:range rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
        <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/ont/geosparql"/>
        <rdfs:isDefinedBy>A spatial representation for a given feature.</rdfs:isDefinedBy>
        <rdfs:label>hasGeometry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/openadr#sendsEvent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/openadr#sendsEvent"/>
    


    <!-- https://w3id.org/def/saref4city#isDerivedFrom -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/saref4city#isDerivedFrom">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/saref4city"/>
        <rdfs:label>is derived from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#consistsOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#consistsOf">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/saref"/>
        <rdfs:label>consists of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#hasFeatureOfInterest -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasFeatureOfInterest">
        <rdfs:isDefinedBy>https://w3id.org/saref#</rdfs:isDefinedBy>
        <rdfs:label>has feature of interest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#isAbout -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isAbout">
        <rdfs:comment>A relationship identifying what an entity, such as a Profile, is about</rdfs:comment>
        <rdfs:isDefinedBy>https://w3id.org/saref#</rdfs:isDefinedBy>
        <rdfs:label>isAbout</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#isMeasuredIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isMeasuredIn">
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>is measured in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#isRelatedToMeasurement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isRelatedToMeasurement">
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>is related to measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#makesMeasurement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#makesMeasurement">
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>makes measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#measuresProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#measuresProperty">
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>measures property</rdfs:label>
    </owl:ObjectProperty>
    


